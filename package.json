{
  "name": "pokemon-graphql-schema",
  "version": "0.1.3",
  "type": "module",
  "description": "Exports of typeDefs and resolvers of Pok√©API GraphQL service to be used with graphql-tools",
  "keywords": [
    "graphql",
    "pokemon",
    "pokeapi"
  ],
  "main": "./dist/index-cjs.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index-cjs.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "git@github.com:hoangvvo/pokemon-graphql-schema.git",
  "author": "Hoang Vo <40987398+hoangvvo@users.noreply.github.com>",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "^15.3.0"
  },
  "dependencies": {
    "@graphql-tools/merge": "^8.3.3",
    "@graphql-tools/schema": "^9.0.1"
  },
  "scripts": {
    "format": "npx prettier --write ./src",
    "build": "rollup -c && cp src/index.d.ts dist"
  },
  "devDependencies": {
    "@rollup/plugin-graphql": "^1.1.0",
    "prettier": "^2.7.1",
    "rollup": "^2.77.3"
  }
}
